public class LCMCalculator {
    public static long gcd(long a, long b) {
        while (b != 0) {
            long temp = b;
            b = a % b;
            a = temp;
        }
        return a;
    }
    public static long lcm(long a, long b) {
        return (a / gcd(a, b)) * b; 
    }

    public static void main(String[] args) {
        long N = 4;
        long M = 6;

        long result = lcm(N, M);
        System.out.println("LCM of " + N + " and " + M + " is: " + result);
    }
}
