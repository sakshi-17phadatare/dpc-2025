public class DivisorCounter {
    public static int countDivisors(int N) {
        int count = 0;
        for (int i = 1; i * i <= N; i++) {
            if (N % i == 0) {
                count++; // i is a divisor
                if (i != N / i) count++; // N/i is a distinct divisor
            }
        }
        return count;
    }

    public static void main(String[] args) {
        int N = 12;
        System.out.println("Total divisors of " + N + ": " + countDivisors(N));
    }
}
