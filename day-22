import java.util.*;

public class FirstKRepeats {
    public static int findFirstKRepeat(int[] arr, int k) {
        Map<Integer, Integer> freqMap = new LinkedHashMap<>();

        // Count frequencies
        for (int num : arr) {
            freqMap.put(num, freqMap.getOrDefault(num, 0) + 1);
        }

        // Find first element with frequency == k
        for (int num : arr) {
            if (freqMap.get(num) == k) {
                return num;
            }
        }

        return -1; // No element found
    }

    public static void main(String[] args) {
        int[] arr = {3, 1, 4, 4, 5, 2, 6, 1, 4};
        int k = 2;
        System.out.println(findFirstKRepeat(arr, k)); // Output: 1
    }
}
